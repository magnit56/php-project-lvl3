{
    "name": "hexlet/code",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "../docker-laravel/vendor/fideloper/proxy": "^4.4",
        "../docker-laravel/vendor/fruitcake/laravel-cors": "^2.0",
        "../docker-laravel/vendor/guzzlehttp/guzzle": "^7.0.1",
        "../docker-laravel/vendor/laravel/framework": "^8.40",
        "../docker-laravel/vendor/laravel/tinker": "^2.5"
    },
    "require-dev": {
        "../docker-laravel/vendor/facade/ignition": "^2.5",
        "../docker-laravel/vendor/fakerphp/faker": "^1.9.1",
        "../docker-laravel/vendor/laravel/sail": "^1.0.1",
        "../docker-laravel/vendor/mockery/mockery": "^1.4.2",
        "../docker-laravel/vendor/nunomaduro/collision": "^5.0",
        "../docker-laravel/vendor/phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
